var classMaterias= function(){}
classMaterias.prototype.setClave(clave)= function()
{
	this.clave=clave;
}
classMaterias.prototype.getClave()= function()
{
	return clave;
}
classMaterias.prototype.setNombre(nombre)= function()
{
	this.nombre=nombre;
}
classMaterias.prototype.getNombre()= function()
{
	return nombre;
}
classMaterias.prototype.setTurno(turno)= function()
{
	this.turno=turno;	
}
classMaterias.prototype.getTurno()= function()
{
	return turno;
}
classMaterias.prototype.setCreditos(creditos)= function()
{
	this.creditos=creditos;
}
classMaterias.prototype.getCreditos()= function()
{
	return creditos;
}
classMaterias.prototype.setMaestro(maestro)= function()
{
	this.maestro=maestro;
}
classMaterias.prototype.getMaestro()= function()
{
	return maestro;
}
//////////
var classProcesos=function(){}
classProcesos.prototype.ask(question,format,callback)
{
	var stdin=process.stdin;
	var stdout=process.stdout;
	stdin.resume();
	stdin.once('data',function(data)
		{
			data=data.toString().trim();
			if(format.test(data))
			{
				callback(data);
			}
			else
				stdout.write("No cumple con el formato "+format+" \n")
				ask(question,format,callback);
		});
}
//////////
var classModule=function(){}
classModule.prototype.classProcesos();
classModule.prototype.classMaterias();
classModule.ask("Clave: ",/.+/,classModule.getClave()
